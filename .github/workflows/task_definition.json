{
  "family": "DemoGitActions",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::010526242429:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::010526242429:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "iris_pred",
      "image": "010526242429.dkr.ecr.us-east-2.amazonaws.com/iris_pred:latest",
      "essential": true,
      "portMappings": [
        {
          "name": "iris_pred_8000",
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/DemoGitActions",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true",
          "mode": "non-blocking",
          "max-buffer-size": "25m"
        }
      },
      "systemControls": []
    }
  ],
  "volumes": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
